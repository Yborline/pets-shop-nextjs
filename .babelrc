{
  "env": {
    "development": {
            "presets": ["next/babel"],
      "plugins": ["inline-react-svg"]
    },
    "production": {
            "presets": ["next/babel"],
      "plugins": [           "inline-react-svg",
        [
          "babel-plugin-styled-components",
          { "ssr": true, "displayName": true, "preprocess": false }
        ]
      ],

    },
    "test": {
      "presets": ["next/babel"]
    }
  },
  "plugins": [
    [
      "babel-plugin-styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ]
  ]


}



